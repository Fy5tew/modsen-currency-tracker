    "config": {
        "wpdev": "--config webpack.dev.js",
        "wpprod": "--config webpack.prod.js",
        "jest": "--config jest.config.ts"
    },
    "scripts": {
        "start-dev": "cross-env-shell webpack serve ${npm_package_config_wpdev}",
        "start-prod": "cross-env-shell webpack serve ${npm_package_config_wpprod}",
        "build-dev": "cross-env-shell webpack ${npm_package_config_dev}",
        "build-prod": "cross-env-shell webpack ${npm_package_config_prod} --stats-children",
        "clean": "rd /s /q build",
        "lint": "eslint .",
        "lint-fix": "eslint src --fix",
        "format": "prettier . --write",
        "test": "cross-env jest --passWithNoTests ${npm_package_config_jest}",
